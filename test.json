{
  "rust-analyzer.vfs.extraIncludes": [],
  "rust-analyzer.statusBar.showStatusBar": {
    "documentSelector": [
      {
        "language": "rust"
      },
      {
        "pattern": "**/Cargo.toml"
      },
      {
        "pattern": "**/Cargo.lock"
      },
      {
        "pattern": "extension-output-rust-lang.rust-analyzer*",
        "scheme": "output"
      }
    ]
  },
  "rust-analyzer.server.extraEnv": {},
